<module>
  <inherits name='arez.Arez'/>
  <inherits name='arez.component.Component'/>

  <source path='' includes='*.java'/>

  <!--
    These properties are all defined in arez.table.TableConfig
    See that class for a better understanding of what these properties do.
  -->

  <define-property name='arez.table.environment' values='development,production'/>
  <set-property name='arez.table.environment' value='production'/>

  <define-property name='arez.table.check_api_invariants' values='true,false'/>
  <set-property name='arez.table.check_api_invariants' value='false'/>

  <define-property name='arez.table.logger' values='console,proxy,none'/>
  <set-property name='arez.table.logger' value='console'/>
</module>
